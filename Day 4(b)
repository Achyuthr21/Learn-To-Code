Write a C Program to find the maximum and minimum from three user inputted numbers
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if((a>b)&&(a>c))
    {
        if(b>c)
        {
            printf("%d\n",a);
            printf("%d",c);
        }
        else
        {
            printf("%d\n",a);
            printf("%d",b);
        }
    }
   else if((b>a)&&(b>c))
    {
        if(a>c)
        {
            printf("%d\n",b);
            printf("%d",c);
        }
        else
        {
         printf("%d\n",b);
         printf("%d",a);
        }
    }
    else
    {
        if(a>b)
        {
            printf("%d\n",c);
            printf("%d",b);
        }
        else
        {
            printf("%d\n",c);
            printf("%d",a);
        }
    }
return 0;
}
